  "author": "Pup Cid",
  "entry": "main.js",
  "enabled": false,
  "permissions": [
    "socket.io",
    "routes",
    "tiktok-events",
    "database",
    "file-upload"
  ],
  "dependencies": [],
  "features": [
    "webgpu-rendering",
    "rendergraph-architecture",
    "structure-of-arrays",
    "triple-buffering",
    "worker-physics",
    "xoroshiro128-rng",
    "texture-atlas",
    "motion-trails",
    "gamma-correct-blending",
    "context-loss-recovery",
    "adaptive-performance",
    "custom-emoji-sets",
    "user-mappings",
    "file-upload",
    "superfan-burst",
    "flow-integration",
    "obs-hud-support",
    "database-config",
    "localization",
    "gift-triggered",
    "like-triggered",
    "follow-triggered",
    "share-triggered",
    "persistent-storage"
  ]
}
